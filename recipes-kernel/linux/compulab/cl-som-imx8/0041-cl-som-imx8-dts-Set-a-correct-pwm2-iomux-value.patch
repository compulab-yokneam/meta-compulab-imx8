+++ b/arch/arm64/boot/dts/compulab/cl-som-imx8.dts
@@ -208,7 +208,7 @@
 
 		pinctrl_pwm2: pwm2grp {
 			fsl,pins = <
-				MX8MQ_IOMUXC_GPIO1_IO13_PWM2_OUT	0x16
